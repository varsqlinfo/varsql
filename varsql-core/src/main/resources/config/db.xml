<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dataSource SYSTEM  "../dtd/connection.config.dtd">

<dataSource name="varsql">
	<type>{{type}}</type>
	<driver>{{driverClass}}</driver>
	<url>{{{url}}}</url>
	<username>{{username}}</username>
	<password>{{pw}}</password>
	<initial_size>5</initial_size>
	<max_active>10</max_active>
	<min_idle>3</min_idle>
	<max_idle>5</max_idle>
	<timebetweenevictionrunsmillis>60000</timebetweenevictionrunsmillis>
	<test_while_idle>true</test_while_idle>
	<pool_option></pool_option>
	<connection_option>sslConnection=true</connection_option>
</dataSource>